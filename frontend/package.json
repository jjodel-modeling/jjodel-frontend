  },
  "erasable_comments": {
    "commentoldversions": "node: 16.15.0, npm:10.9.2 react 16?",
    "commentnewversions": "node: 23.11.0, npm:10.9.2 react 18",
    "upgrading": [
      "check new node version, use nvm install <nodeversion>",
      "nvm use <nodeversion>",
      "npm i -g npm@latest",
      "npm outdated",
      "for the outdateds do npm update <pkg>@latest",
      "if you do just 'npm update', it will update only non breaking version numbers (the second one?)",
      "npm install -g npm-check-updates",
      "ncu --upgrade",
      "downgrade webpack-cli to < 5 (missing node polyfills) or replace jsx-transform",
      "downgrade react-scripts to < 5 (v5 has an issue as it excludes support for some node features and polyfills)",
      ""
    ]
